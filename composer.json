{
  "name": "adoxography/disambiguatable",
  "description": "Disambiguatable trait for Laravel models, to distinguish between identical lists of columns",
  "type": "library",
  "require": {
    "php": ">=7.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Graham Still",
      "email": "gstill@uw.edu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Adoxography\\Disambiguatable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adoxography\\Disambiguatable\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adoxography\\Disambiguatable\\DisambiguatableServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^5.3",
    "phpunit/phpunit": "^9.2"
  }
}
